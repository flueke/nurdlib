log_level = info # verbose, debug, or spam.

# Name and crate ID number.
CRATE("MY_VME", 7) {
	GSI_TRIDI(0x02000000) {
		trlo2_timestamper = true
	}
	GSI_VFTX2(32, 0x08000000) {
		channel_enable = 0..19
	}
	GSI_VFTX2(16, 0x09000000) {
		GATE {
			time_after_trigger = -2us
			width = 1us
		}
	}
}
